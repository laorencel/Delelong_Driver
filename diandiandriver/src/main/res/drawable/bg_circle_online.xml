<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval"
            android:useLevel="false">
        <solid android:color="@color/colorPrimary" />
        <padding
            android:left="15dp"
            android:top="15dp"
            android:right="15dp"
            android:bottom="15dp" />
        <solid
            android:color="#Fe8a03" />
        <!--<stroke-->
            <!--边框-->
            <!--android:width="0.2dp"-->
            <!--android:color="@color/colorGray" />-->
        <size android:width="50dp"
            android:height="50dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/colorPrimary" />
            <padding
                android:left="15dp"
                android:top="15dp"
                android:right="15dp"
                android:bottom="15dp" />
            <solid
                android:color="#ffc888" />
            <size android:width="50dp"
                android:height="50dp" />
        </shape>
    </item>
</selector>